{
	"name": "@syncblock/uikit-react-extension",
	"version": "0.0.1",
	"description": "React components extension for UIkit",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./scss": "./src/scss/index.scss",
		"./scss/variables": "./src/scss/variables.scss",
		"./scss/mixins": "./src/scss/mixins.scss"
	},
	"files": [
		"src"
	],
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "tsc",
		"dev": "tsc --watch",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"uikit": "^3.23.12"
	},
	"devDependencies": {
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"bun-types": "latest",
		"typescript": "~5.8.3"
	},
	"keywords": [
		"react",
		"uikit"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/syncblock/uikit-react-extension"
	}
}
